<% @ini.each do |section,group| %>
<%= "[#{section}]" if section %>
<%= group.each do |key,v| %>
<%= case v
   when Hash
    line = ""
    line += "# " + v['comment'] + "\n" if v['comment']
    line += ';' if v['disable']
    line += v['value']
   else
    v
   end
%>

<% end %>

<% end %>
